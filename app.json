{
  "expo": {
    "name": "FriendlyFades",
    "slug": "FriendlyFades",
    "platforms": [
      "ios"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/FriendlyFadesLogo.png",
    "splash": {
      "image": "./images/background.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.company.SquareUpApp",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "extra": {
      "eas": {
        "projectId": "1ae72c85-4b1a-4c51-880e-ed44e633e3e1"
      }
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-dev-launcher",
        {
          "launchModeExperimental": "most-recent"
        }
      ],
      [
        "react-native-permissions",
        {
          "iosPermissions": [
            "LocationWhenInUse",
            "Notifications",
            "PhotoLibrary"
          ]
        }
      ]
    ],
    "android": {
      "package": "com.company.SquareUpApp",
      "googleServicesFile": "./GoogleService-Info.plist"
    }
  }
}
